{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "loading-datafactory-full-incremental"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabaseAzure_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseAzure'"
		},
		"AzureKeyVaultSqlPassword_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sql-account-key.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "loading-sql-server-new.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "loadind-sql-db"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"AzureSqlDatabaseAzure_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "loading-sql-server-new.database.windows.net"
		},
		"AzureSqlDatabaseAzure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "my_db_azure"
		},
		"AzureSqlDatabaseAzure_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "loading-datafactory-full-incremental_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"AzureSqlDatabaseAzure_password": {
						"value": "[parameters('AzureSqlDatabaseAzure_password')]"
					},
					"AzureKeyVaultSqlPassword_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultSqlPassword_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabaseAzure_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseAzure_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseAzure_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseAzure_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseAzure_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseAzure_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}